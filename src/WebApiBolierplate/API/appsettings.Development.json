{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AppConfig": {
    "DataBase": {
      "ConnectionString": "sample connection string"
    },
    "Logger": {
      "DateFormat": "yyyy-MM-dd HH:mm:ss.fff",
      "FileName": "ErrorLog",
      "RelativePath": "",
      "ReplacementValue":  ";"
    },
    "JWT": {
      "Audience": "https://www.example.com",
      "Issuer": "WebApiBoilerplate",
      "Key": "This is a JWT secret key",
      "DaysValid": 30
    }
  }
}
